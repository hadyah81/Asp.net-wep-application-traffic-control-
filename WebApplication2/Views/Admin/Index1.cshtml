@model IEnumerable<WebApplication2.Models.Customer>
@{

    Layout = "AdminLayout";
}

<table class="table">
    <thead>
        <tr>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">

                    @Html.DisplayNameFor(model => model.Fname)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Lname)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Gender)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Ssn)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Nationality)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Licensenumber)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.LicenseCategory)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.LicensingCenter)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Contactnumber)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.Email)
                </a>
            </td>
            <td>
                <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                    @Html.DisplayNameFor(model => model.ImagePath)

            </td>
            <td></td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Fname)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Lname)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Gender)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Ssn)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Nationality)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Licensenumber)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.LicenseCategory)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.LicensingCenter)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Contactnumber)
                    </a>
                </td>
                <td>
                    <a class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">
                        @Html.DisplayFor(modelItem => item.Email)
                    </a>
                </td>
                <td>
                    @*    @Html.DisplayFor(modelItem => item.ImagePath)*@
                    <img src="@Url.Content("~/Images/" + item.ImagePath )" asp-append-version="true" width="100" height="100" />
                    <br>
                </td>

                <td>

                    <a asp-action="Details" asp-route-id="@item.Id" class="btn btn-outline-primary btn-sm mb-0 me-3" target="_blank">Details</a>

                </td>
            </tr>
        }
    </tbody>
</table>
